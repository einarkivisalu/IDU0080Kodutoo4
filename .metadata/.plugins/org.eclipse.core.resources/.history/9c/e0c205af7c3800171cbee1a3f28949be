public class Tester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		OrderService_OrderServicePort_Client Client = new OrderService_OrderServicePort_Client();
		try {
			printProducts(Client.getProductsFromMyService());
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
	public static void printOrders(List<ttu.idu0080.myclient.Order> orders)	{
			if (orders != null)
			{
			for (ttu.idu0080.order.server.Product p: products) {
			System.out.println("product id:" + p.getProduct() + " name:" + p.getName() );
	}

}
